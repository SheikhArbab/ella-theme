                            <div class="site-nav-list "><a href="{{ link.url }}" class="site-nav-title menu-lv-2"><span>{{ 'general.navigation.go_to' | t }} {{ link.title | escape }}</span></a></div>
                        </div>
                        <div class="menu-dropdown__column menu-dropdown__column-{{ column }} w-auto{% unless show_banner_1 or show_product %} column-full{% endunless %} {% unless show_product %}no_show_product{% endunless %}{% unless show_banner_1 %} no_show_banner{% endunless %}" data-masonry='{ "itemSelector": "[data-gridItem]", "columnWidth": ".grid-sizer" }'>
                            <div class="grid-sizer"></div>
                            {%- for childlink in link.links -%}
                                {% assign childtitle = childlink.title | escape | downcase | handle %}
                                <div class="site-nav dropdown{% if show_label_blink %} menu-lv-blink{% endif %}" data-gridItem>
                                    <div class="site-nav-list">
                                        <a href="{{ childlink.url }}" class="site-nav-title{% if childlink.links != blank %} menu_mobile_link_2{% endif %}{% if sale_item_lv2 contains childtitle %} active-sale{% endif %}"{% if childlink.current %} aria-current="page"{% endif %}>
