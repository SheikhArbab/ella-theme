                                                        <li class="site-nav-item menu-lv-3 text-left navigation_all_mobile d-none"><a href="{{ childlink.url }}" class="site-nav-link m-zero link link-underline link--text focus-inset link-effect caption-large"><span class="text p-relative">{{ 'general.navigation.go_to' | t }} {{ childlink.title | escape }}</span></a></li>
                                                        {%- for grandchildlink in childlink.links -%}
                                                            {% assign grandchildtitle = grandchildlink.title | escape | downcase | handle %}
                                                            <li class="site-nav-item text-left{% if sale_item_lv3 contains grandchildtitle %} active-sale{% endif %}"><a href="{{ grandchildlink.url }}" class="site-nav-link m-zero link link--text focus-inset link-effect caption-large"{% if grandchildlink.current %} aria-current="page"{% endif %}><span class="text{% if grandchildlink.current %} site-nav-active{% endif %} p-relative">{{ grandchildlink.title | escape }}</span>
                                                                    {%- if new_label_3 contains grandchildtitle -%}
                                                                        <span class="label new-label">{{ 'general.navigation.new_label' | t }}</span>
                                                                    {%- elsif sale_label_3 contains grandchildtitle -%}
                                                                        <span class="label sale-label">{{ 'general.navigation.sale_label' | t }}</span>
                                                                    {%- elsif hot_label_3 contains grandchildtitle -%}
                                                                        <span class="label hot-label">{{ 'general.navigation.hot_label' | t }}</span>
                                                                    {%- endif -%}
                                                                </a></li>
                                                        {%- endfor -%}
                                                    </ul>
                                                </div>
                                            {%- endif -%}
                                        </div>
                                    </div>
                                {%- endfor -%}
                            </div>
                            {%- if menu_text_left != blank -%}</div>{%- endif -%}
                        </div>
                        {% if mega_tyle == 'style2' or mega_tyle == 'style3' and show_banner_1 %}
                            {%- if show_banner_1 -%}
                            <div class="menu-banner__top">
                                <div class="site-nav-banner--item site-nav-banner-item--right d-inline-block ver-alg-top w-100">    
                                    <div class="banner d-block">
                                        <a{% if banner_url_1 == blank %} role="link" aria-disabled="true"{% else %} href="{{ banner_url_1 }}"{% endif %} class="image adaptive_height image-zoom w-100 text-center" {% if show_banner_1 %}style="padding-top: {{ 1 | divided_by: banner_1.aspect_ratio | times: 100 }}%"{% endif %} title="{{ banner_1.alt | default: shop.name | escape }}">
                                            {%- if banner_1 != blank and show_banner_1 -%}
                                                <img {% if settings.enable_lazyload %}data-{% endif %}srcset="{{ banner_1 | img_url: 'master' }}"
                                                {% unless settings.enable_lazyload %}src="{{ banner_1 | img_url: 'master' }}"{% endunless %} loading="lazy" alt="{{ banner_1.alt | default: shop.name | escape }}"{% if settings.enable_lazyload %} class="lazyload"{% endif %}> {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                                            {%- else -%}
                                                <div class="not_img" style="--height_not_img: 337px">210 x 337px</div>
                                            {%- endif -%}
                                        </a>
                                    </div>
                                </div>
                            </div>
                            {%- endif -%}
                        {% endif %}
                        <div class="menu-dropdown__banner clearfix {% unless show_product %}no_show_product{% endunless %}{% unless show_banner_1 %} no_show_banner{% endunless %}">
                            <div class="site-nav-banner d-flex ft-0 ls-0 flex-jc-end background-bottom" style="--mega-menu-bottom-background: {{ bg_mega_menu_bottom}}">
                                {%- if show_product -%}
                                    <div class="menu-dropdown__product col-left">
                                        {% if product_title != blank %}<h3 class="menu-dropdown__block--title text-center p-relative"><span class="text d-inline-block ver-alg-top w-auto p-zero p-relative zi-2">{{ product_title | escape }}</span></h3>{% endif %}
                                        <div class="menu-dropdown__block--content">
                                            <div class="products-grid column-{{product_limit}} products-carousel" data-row="{{product_limit}}">
                                                {% for product in collections[collection_product].products limit: product_limit %}
                                                    <div class="product">{% render 'mega-menu-product-card' product_card_product: product, media_size: 'adapt' %}</div>
                                                {% endfor %}
                                            </div>
                                        </div>
                                    </div>
                                {%- endif -%}
                                {%- if show_banner_1 and mega_tyle == 'style1' or mega_tyle == 'style4' -%}
                                    <div class="site-nav-banner--item site-nav-banner-item--right d-inline-block ver-alg-top w-100">    
                                        <div class="banner d-block">
                                            <a{% if banner_url_1 == blank %} role="link" aria-disabled="true"{% else %} href="{{ banner_url_1 }}"{% endif %} class="image adaptive_height image-zoom w-100 text-center" style="padding-top: {{ 1 | divided_by: banner_1.aspect_ratio | times: 100 }}%" title="{{ banner_1.alt | default: shop.name | escape }}">
                                                {%- if banner_1 != blank -%}
                                                    <img {% if settings.enable_lazyload %}data-{% endif %}srcset="{{ banner_1 | img_url: 'master' }}" {% unless settings.enable_lazyload %}src="{{ banner_1 | img_url: 'master' }}"{% endunless %} loading="lazy" alt="{{ banner_1.alt | default: shop.name | escape }}"{% if settings.enable_lazyload %} class="lazyload"{% endif %}> {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                                                {%- else -%}
                                                    <div class="not_img" style="--height_not_img: 337px">210 x 337px</div>
                                                {%- endif -%}
                                            </a>
                                        </div>
                                    </div>
                                {%- endif -%}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        {%- endif -%}
    {%- else -%}
        <a href="{{ link.url }}" class="menu-lv-1__action header__menu-item header__menu-item list-menu__item link link--text focus-inset"{% if link.current %} aria-current="page"{% endif %}>
            <span class="text{%- if link.current %} header__active-menu-item{% endif %} link-effect">{{ link.title | escape }}</span>
        </a>
    {%- endif -%}
</li>