                            <div class="site-nav-banner d-flex ft-0 ls-0 flex-jc-end">
                                {%- if show_product -%}
                                    <div class="menu-dropdown__product col-left">
                                        {% if product_title != blank %}<h3 class="menu-dropdown__block--title text-center p-relative"><span class="text d-inline-block ver-alg-top w-auto p-zero p-relative zi-2">{{ product_title | escape }}</span></h3>{% endif %}
                                        <div class="menu-dropdown__block--content">
                                            <div class="site-nav-product">{% render 'mega-menu-product-card' product_card_product: product, media_size: image_ratio %}</div>
                                        </div>
                                    </div>
                                {%- endif -%}
                                {%- if show_banner_1 -%}
                                    <div class="site-nav-banner--item site-nav-banner-item--right d-inline-block ver-alg-top w-100">    
                                        <div class="banner d-block">
                                            <a{% if banner_url_1 == blank %} role="link" aria-disabled="true"{% else %} href="{{ banner_url_1 }}"{% endif %} class="image adaptive_height image-zoom w-100 text-center" style="padding-top: {{ 1 | divided_by: banner_1.aspect_ratio | times: 100 }}%" title="{{ banner_1.alt | default: shop.name | escape }}">
                                                {%- if banner_1 != blank -%}
                                                    <img {% if settings.enable_lazyload %}data-{% endif %}srcset="{{ banner_1 | img_url: 'master' }}" {% unless settings.enable_lazyload %}src="{{ banner_1 | img_url: 'master' }}"{% endunless %} loading="lazy" alt="{{ banner_1.alt | default: shop.name | escape }}"{% if settings.enable_lazyload %} class="lazyload"{% endif %}>{% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                                                {%- else -%}
                                                    <div class="not_img" style="--height_not_img: 337px">210 x 337px</div>
                                                {%- endif -%}
                                            </a>
                                        </div>
                                    </div>
                                {%- endif -%}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
