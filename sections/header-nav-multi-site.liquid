<link rel="stylesheet" href="{{ 'component-megamenu.css' | asset_url }}" media="all" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'component-header-nav-multi-site.css' | asset_url }}" media="all" onload="this.media='all'">
<noscript>{{ 'component-megamenu.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'component-header-nav-multi-site.css' | asset_url | stylesheet_tag }}</noscript>
<style>
    {%- liquid
        assign header_top_color = section.settings.header_top_color
        assign header_bottom_bg = section.settings.header_bottom_bg
        assign header_bottom_bg_gradient = section.settings.header_bottom_bg_gradient
        assign font_size_customer = section.settings.font_size_customer | append: 'px'
        assign search_input_color = section.settings.search_input_color
        assign search_input_bg_color = section.settings.search_input_bg_color
        assign search_input_border = section.settings.search_input_border
        assign input_font_size = section.settings.input_font_size | append: 'px'
        assign input_text_transform = section.settings.input_text_transform
        assign input_font_weight = section.settings.input_font_weight
        assign cart_color_item = section.settings.cart_color_item
        if section.settings.cart_bg_item_gradient != blank and section.settings.cart_bg_item_gradient != 'rgba(0,0,0,0)'
            assign cart_bg_item = section.settings.cart_bg_item_gradient
        else
            assign cart_bg_item = section.settings.cart_bg_item
        endif

        assign logo_width = section.settings.logo_width
        assign bg_tab_home = section.settings.bg_tab_home
        assign bg_tab_home_gradient = section.settings.bg_tab_home_gradient
        assign tab_font_size = section.settings.tab_font_size | append: 'px' 
        assign tab_font_size_mb = section.settings.tab_font_size_mb | append: 'px'
        assign tab_font_weight_mb = section.settings.tab_font_weight_mb
        assign color_border_separation = section.settings.color_border_separation
    -%}
    .header-nav-multi-site .header__location {color: {{ header_top_color }};font-size: {{ font_size_customer }}}.header-nav-multi-site .search_details .search-modal__form .search__input {color: {{ search_input_color }};background-color: {{ search_input_bg_color }};border: 1px solid {{ search_input_border }};font-weight: {{ input_font_weight }};font-size: {{ input_font_size }};text-transform: {{ input_text_transform }}}.header-nav-multi-site .search_details .search-modal__form ::-webkit-input-placeholder {color: {{ search_input_color }};font-weight: {{ input_font_weight }};font-size: {{ input_font_size }};text-transform: {{ input_text_transform }}}.header-nav-multi-site .search_details .search-modal__form ::-moz-placeholder {color: {{ search_input_color }};font-weight: {{ input_font_weight }};font-size: {{ input_font_size }};text-transform: {{ input_text_transform }}}.header-nav-multi-site .search_details .search-modal__form :-ms-input-placeholder {color: {{ search_input_color }};font-weight: {{ input_font_weight }};font-size: {{ input_font_size }};text-transform: {{ input_text_transform }}}.header-nav-multi-site .search_details .search-modal__form :-moz-placeholder {color: {{ search_input_color }}}.header-nav-multi-site .search_details .search-modal__form .search__button {background: {% if section.settings.search_button_bg_color_gradient != blank %}{{ section.settings.search_button_bg_color_gradient }}{% else %}{{ section.settings.search_button_bg_color }}{% endif %};color: {{ section.settings.search_button_color }};border: 1px solid {{ section.settings.search_button_border }}}.header-nav-multi-site .header-top-right-group .customer-service-text {color: {{ header_top_color }}}.header-nav-multi-site .cart-text, .header-nav-multi-site .customer-links,.header-nav-multi-site .wishlist-text, .header-nav-multi-site .customer-links a {color: {{ header_top_color }}}.header-nav-multi-site .header__icon--cart .cart-count-bubble,.bottom-bar--list .cart-count-bubble,.header-nav-multi-site .header__icon--wishlist .wishlist-count-bubble {background: {{ cart_bg_item }};color: {{ cart_color_item }}}.bottom-bar--list .cart-count-bubble .text,.header-nav-multi-site .header__icon--wishlist .wishlist-count-bubble .text {color: {{ cart_color_item }}}.header-nav-multi-site .wrapper-header-bottom{ {%- if header_bottom_bg != 'rgba(0,0,0,0)' and header_bottom_bg != blank -%}background: {%- if header_bottom_bg_gradient != blank -%}{{ header_bottom_bg_gradient }}{%- else -%}{{ header_bottom_bg }}{%- endif -%}{%- endif -%}}
    {%- if header_top_color != 'rgba(0,0,0,0)' and header_top_color != blank -%}
        .header-nav-multi-site .header__icon .icon{fill: {{ header_top_color }}}.header-nav-multi-site .live-wrapper-icon svg {stroke: {{ header_top_color }}}
    {%- endif -%}
    .header-nav-multi-site .wrapper-header-bottom .header__icon .icon {fill: {{ section.settings.lv_1_color }}}.header-nav-multi-site .wrapper-header-bottom .cart-count-bubble {background-color: {{ section.settings.lv_1_color }};color: {{ header_bottom_bg }}}.header-nav-multi-site--style_2 .wrapper-header-bottom .cart-count-bubble {background: {{ cart_bg_item }}}.scrolled-past-header .header-nav-multi-site .menu--mobile-lv-1__action > .icon{fill: {{ section.settings.lv_1_color }}}.header-nav-multi-site .menu-drawer__close-button{font-size: {{ section.settings.lv_1_font_size | append: 'px' }};font-weight: {{ section.settings.lv_1_font_weight }};color: {{ section.settings.lv_1_color }};text-transform: {{ section.settings.lv_1_text_transform }}}
    /* Settings Header Tab */
    .header-nav-multi-site .header-top--wrapper{background: {% if bg_tab_home_gradient != 'rgba(0,0,0,0)' and bg_tab_home_gradient != blank %}{{ bg_tab_home_gradient }}{% else %}{{ bg_tab_home }}{% endif %}}.header-nav-multi-site .header-top--left .header__list li svg {height: {{ tab_font_size }};width: auto}.header-nav-multi-site .header-top--left .header__list .item--border:before {background-color: {{ color_border_separation }}}
    {%- if section.settings.header_style == 'style_2' -%}
        .header-nav-multi-site .halo-productBadges {top: 15px}.header-nav-multi-site .halo-productBadges--right {right: 15px}.header-nav-multi-site .halo-productBadges--left {left: 15px}.header-nav-multi-site .card-review {line-height: 24px}.product-block__banner .product-block__cap--text {font-style: italic}
    {%- endif -%}
    {%- if settings.enable_layout_rtl -%}
    .layout_rtl .header-nav-multi-site .header__list .item--border:not(.is-active):before {right: auto;left: 0}.layout_rtl .header-nav-multi-site .header__icon+.header__icon, .layout_rtl .header-nav-multi-site .header__iconItem+.header__iconItem, .layout_rtl .header-nav-multi-site .header__languageCurrency {margin-left: 0;margin-right: 20px}.layout_rtl .header-nav-multi-site .header__group .cart-count-bubble {margin-right: 0}.layout_rtl .header-nav-multi-site .halo-top-currency .dropdown-toggle .text {margin-left: 5px;margin-right: 5px}.layout_rtl .header-nav-multi-site .live_help,.layout_rtl .header-nav-multi-site .customer-links {margin-left: 0;margin-right: 20px}
    {%- endif -%}
    @media screen and (max-width: 1024px) {
        .header-nav-multi-site .header-top--wrapper .header__list a {padding: var(--padding_vertical_mb) var(--padding_hozi_mb);font-size: {{ tab_font_size_mb }};font-weight: {{ tab_font_weight_mb }}}.header-nav-multi-site .menu-lv-item.menu-lv-1:hover > a > .text, .header-nav-multi-site .menu-lv-item.menu-lv-2:hover > a > .text, .header-nav-multi-site .menu-lv-item.menu-lv-3:hover > a > .text{border-bottom: 0}.bottom-bar--list .cart-count-bubble {min-width: 20px;min-height: 20px}.bottom-bar--list .cart-count-bubble .text {border-radius: 50%;line-height: 20px;font-size: var(--font-size)}.header-nav-multi-site .cart-text, .header-nav-multi-site .customer-links, .header-nav-multi-site .wishlist-text, .header-nav-multi-site .customer-links a{color: #232323 !important}.halo-sidebar_menu .header__inline-menu{display: none} .halo-sidebar_menu .header__inline-menu.is-active{display: block}
    }
    @media (max-width: 992px) {
        {%- if section.settings.header_style == 'style_2' -%}
            .product-block-has__banner .product-block__banner.pd--style2 figure {max-width: 100% !important}.product-block-has__banner .product-block__banner,.custom-image-banner-block {overflow: hidden}.product-block-has__banner .product-block__banner .product-block__cap.align-items--bottom {transform: translate(-30%,-78%)}.style-dots-2 .halo-product-block .slick-dots {bottom: -65px}.style-dots-2 .halo-row .slick-dots {bottom: -50px !important}
        {%- endif -%}
    }
    @media screen and (min-width: 1025px) {
        .header-nav-multi-site .header-top--right{width: calc(100% - ({{ logo_width | append: 'px' }}))}.header-nav-multi-site .header-bottom .header__logo a{width: var(--width_logo);height: var(--height_logo)}
        .header-nav-multi-site .wrapper-header-bottom .header-bottom-left svg{width: {{ logo_width | append: 'px' }}}.header-nav-multi-site .header__heading .h2{font-size: {{ section.settings.logo_font_size  | append: 'px' }};font-weight: {{ section.settings.logo_font_weight }};{%- if section.settings.logo_color != 'rgba(0,0,0,0)' and section.settings.logo_color != blank -%}color: {{ section.settings.logo_color }}{%- endif -%};white-space: normal}
        {%- if section.settings.logo_text != blank -%}
            .header-nav-multi-site .header__logo .header__heading-link{ height: {{ logo_width | append: 'px' }}}
        {%- endif -%}
        .header-nav-multi-site .top-language-currency .text {
            {%- if settings.type_body_font_size >= 14 -%}
                font-size: calc(var(--font-body-size) - 2px)
            {%- else -%}
                 font-size: calc(var(--font-body-size))
            {%- endif -%}
        }
        .header-nav-multi-site .header__inline-menu{display: none}.header-nav-multi-site .header__inline-menu.is-active{display: block}
    }
</style>
<script src="{{ 'details-disclosure.js' | asset_url }}" defer="defer"></script>
{%- liquid
    assign menu_tab = section.blocks | where: "type", "tab"
    assign page_handle = page.handle
-%}
<{% if section.settings.sticky_header_type != 'none' %}sticky-header data-sticky-type="{{ section.settings.sticky_header_type }}"{% else %}div{% endif %} class="header-navigation-wrapper header-wrapper">
    <header class="header header-06 header-nav-multi-site{% if section.settings.menu != blank %} header--has-menu{% endif %}{% if section.settings.header_style == 'style_2' %} header-nav-multi-site--style_2{% endif %}" data-id="shopify-section-{{ section.id }}" style="--spacing-l-r: {{ section.settings.padding_full_width }}px; --icon-mobile-spacing: {{ section.settings.header_icon_mobile_spacing | append: 'px' }}">
        <div class="header-top--wrapper flex-align-center pr-30">
            {%- if section.settings.header_style == 'style_2' -%}
                <div class="container container-{{ section.settings.container }}">
            {%- endif -%}
            <div class="header-top--left header__multi-page text-left clearfix" {% if menu_tab.size > 0 %}data-menu-tab{% endif %}>
                {%- if menu_tab.size > 0 -%}
                    <ul class="header__list list-unstyled disable-srollbar d-flex flex-align-center">
                        {%- for block in menu_tab -%}
                            {%- liquid
                                assign multi_page_handle = pages[block.settings.link].handle
                                assign multi_page_url = pages[block.settings.link].url
                                assign multi_page_title = block.settings.menu
                                assign text_color = block.settings.text_color
                                assign text_bg = block.settings.text_bg
                                assign text_color_active = block.settings.text_color_active
                                assign text_bg_active = block.settings.text_bg_active
                                assign padding_vertical = block.settings.padding_vertical | append: 'px'
                                assign padding_hozi = block.settings.padding_hozi | append: 'px'
                                assign padding_vertical_mb = block.settings.padding_vertical_mb | append: 'px'
                                assign padding_hozi_mb = block.settings.padding_hozi_mb | append: 'px'
                                assign class = 'item'
                                    
                                if template.suffix contains 'landing'
                                    if page_handle == multi_page_handle
                                        assign class = 'item is-active'
                                    endif
                                else
                                    if forloop.first
                                        assign class = 'item is-active'
                                    endif
                                endif

                                if multi_page_url != blank
                                    assign url = multi_page_url
                                else
                                    assign url = routes.root_url
                                endif
                            -%}

                            <li class="{{ class }} p-relative{% if section.settings.enable_separation_tab and forloop.last == false %} item--border{% endif %}" data-load-page="{{ multi_page_handle }}">
                                <a class="link" style="--color_text: {{ text_color }};--background_color_text: {{ text_bg }};--color_text_active: {{text_color_active}};--background_color_text_active: {{ text_bg_active }};--tab_font_size: {{ section.settings.tab_font_size | append: 'px' }};--tab_font_weight:{{section.settings.tab_font_weight}};--padding-vertical: {{ padding_vertical }};--padding-horizontal: {{ padding_hozi }};--padding_vertical_mb: {{ padding_vertical_mb }};--padding_hozi_mb: {{ padding_hozi_mb }}" {% if url == blank %} role="link" aria-disabled="true"{% else %} href="{{ url }}"{% endif %}>{{ multi_page_title }}</a>
                            </li>
                        {%- endfor -%}
                    </ul>
                    {%- for block in menu_tab -%}
                        {%- liquid
                            assign multi_page_handle = pages[block.settings.link].handle
                        -%}
                        {%- if request.page_type == 'page' and page_handle == multi_page_handle -%}
                            <script type="text/javascript">
                                window.page_active = '{{ page_handle }}';
                            </script>
                        {%- endif -%}
                    {%- endfor -%}
                {%- endif -%}
            </div>
            {%- if section.settings.header_style == 'style_2' -%}
                <div class="header-top--center text-center">
                    {%- if section.settings.customer_service_text != blank -%}
                        <div class="header__location">{{ section.settings.customer_service_text }}</div>
                    {%- endif -%}
                </div>
            {%- endif -%}
            <div class="header-top--right header__icons text-right clearfix">
                {%- unless section.settings.header_style == 'style_2' -%}
                    {%- if section.settings.customer_service_text != blank -%}
                    <div class="header__location d-inline-block ver-alg-mid">{{ section.settings.customer_service_text }}</div>
                    {%- endif -%}
                {%- endunless -%}
                <div class="header__iconItem header__group">
                    {%- if section.settings.header_style == 'style_2' -%}
                        {%- if section.settings.show_cart -%}
                            <a href="{{ routes.cart_url }}" class="header__icon header__icon--cart link link--text focus-inset" id="cart-icon-bubble"{% if settings.show_quick_cart %} data-cart-sidebar{% endif %}>
                                {% render 'icon-cart-02' %}
                                <span class="cart-text">{{ 'general.cart.shopping_cart' | t }}</span>
                                <div class="cart-count-bubble">
                                    {%- if cart.item_count < 100 -%}
                                        <span class="text" aria-hidden="true" data-cart-count>{{ cart.item_count }}</span>
                                    {%- endif -%}
                                    <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                                </div>
                            </a>
                        {%- endif -%}
                    {%- endif -%}

                    {%- if section.settings.header_style == 'style_2' %}
                        {%- if settings.show_wishlist and section.settings.show_wishlist -%}
                            <a href="{{ pages['wish-list'].url }}" class="header__icon header__icon--wishlist link link--text focus-inset">{% render 'icon-heart-2' %}<span class="wishlist-text">{{ 'wishlist.general.wishlist' | t }}</span></a>
                        {%- endif -%}
                    {%-  endif -%}
                    {%- if section.settings.header_style == 'style_2' -%}
                        {%- if shop.customer_accounts_enabled and section.settings.show_customer -%}
                            {% if routes.account_login_url contains '/account/login' %}
                                {% assign is_contained = true %}
                            {% else %}
                                {% assign is_contained = false %}
                            {% endif %}
                            <div class="customer-links">
                              {%- if customer -%}
                                  <a class="header__link--account" id="customer_logout_link" href="{{ routes.account_logout_url }}">{{ 'customer.log_out' | t }}</a>
                                  <span> / </span>
                                  <a class="header__link--account" href="{{ routes.account_url }}">{{ 'customer.account.my_account' | t }}</a>
                              {%- else -%}  
                                  <a class="header__link--account" id="customer_login_link" href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}"{% if is_contained %} data-open-auth-sidebar{% endif %}>{{ 'customer.sign_in' | t }}</a>
                                  <span class="or-text">{{ 'customer.or' | t }}</span>
                                  <a class="header__link--account" id="customer_register_link" href="{{ routes.account_register_url }}">{{ 'customer.create_account' | t }}</a>
                              {%- endif -%}                                   
                            </div>
                        {%- endif -%}
                    {%- endif -%}

                    {%- unless section.settings.header_style == 'style_2' -%}
                        {%- if shop.customer_accounts_enabled and section.settings.show_customer -%}
                            {% if routes.account_login_url contains '/account/login' %}
                                {% assign is_contained = true %}
                            {% else %}
                                {% assign is_contained = false %}
                            {% endif %}
                            <a href="{{ routes.account_login_url }}" class="header__icon header__icon--account link link--text"{% if is_contained %} data-open-auth-sidebar{% endif %}>
                                {% render 'icon-account-2' %}
                                <span class="visually-hidden">
                                    {%- liquid
                                    if customer
                                    echo 'customer.account_fallback' | t
                                    else
                                    echo 'customer.log_in' | t
                                    endif
                                    -%}
                                </span>
                            </a>                               
                        {%- endif -%}
                    {%- endunless -%}

                    {%- unless section.settings.header_style == 'style_2' -%}
                        {%- if settings.show_wishlist and section.settings.show_wishlist -%}
                            <a href="{{ pages['wish-list'].url }}" class="header__icon header__icon--wishlist link link--text focus-inset">
                                {% render 'icon-heart-2' %}
                                <div class="wishlist-count-bubble"><span class="text" aria-hidden="true" data-wishlist-count></span><span class="visually-hidden">{{ 'wishlist.general.wishlist' | t }}</span></div>
                            </a>
                        {%- endif -%}
                    {%- endunless -%}

                    {%- unless section.settings.header_style == 'style_2' -%}
                        {% if section.settings.show_cart %}
                            <a href="{{ routes.cart_url }}" class="header__icon header__icon--cart link link--text focus-inset" id="cart-icon-bubble"{% if settings.show_quick_cart %} data-cart-sidebar{% endif %}>
                                {% render 'icon-cart' %}
                                <span class="visually-hidden">{{ 'templates.cart.cart' | t }}</span>
                                <div class="cart-count-bubble">
                                    {%- if cart.item_count < 100 -%} 
                                    <span class="text" aria-hidden="true" data-cart-count>{{ cart.item_count }}</span>
                                    {%- endif -%}
                                    <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                                </div>
                            </a>
                        {%- endif -%}
                    {%- endunless -%}

                    {%- if section.settings.ask_an_expert or section.settings.contact_us -%}
                        <div class="live_help header__icon p-relative ml-20">
                            <div class="live-wrapper-icon stroke-icon">{% render 'icon-phone',class:'stroke-w-5' %}</div>
                            <div class="live_help_group p-absolute ml-0 zi-3 right-0 o-hidden opacity-0 h-0">
                                <div class="live_help_group_wrap text-left">
                                {%- if section.settings.ask_an_expert -%}
                                    <div class="live-ask-an-expert">
                                    {%- if section.settings.ask_an_expert_title != blank -%}
                                        <h4>{{ section.settings.ask_an_expert_title }}</h4>
                                    {%- endif -%}
                                    {%- if section.settings.ask_an_expert_desc != blank -%}
                                        <div class="desc">{{ section.settings.ask_an_expert_desc }}</div>
                                    {%- endif -%}
                                    {%- if section.settings.ask_an_expert_button != blank -%}
                                        <button type="button" class="button button-2" title="{{ 'ask_an_expert.general.text' | t}}" data-open-ask-an-expert>{{ section.settings.ask_an_expert_button }}</button>
                                    {%- endif -%}
                                    {%- if section.settings.ask_an_expert_text != blank -%}
                                        <span class="text">{{ section.settings.ask_an_expert_text }}</span>
                                    {%- endif -%}
                                    </div>
                                {%- endif -%}
                                
                                {%- if section.settings.contact_us -%}
                                    <div class="live-contact-us">
                                    {%- if section.settings.contact_us_title != blank -%}
                                        <h4>{{ section.settings.contact_us_title }}</h4>
                                    {%- endif -%}
                                    {%- if section.settings.contact_us_desc != blank -%}
                                        <div class="desc">{{ section.settings.contact_us_desc }}</div>
                                    {%- endif -%}
                                    {%- if section.settings.contact_us_button != blank -%}
                                        <a class="button button-2"{% if section.settings.contact_us_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ section.settings.contact_us_link }}"{% endif %} title="{{ section.settings.contact_us_button }}">{{ section.settings.contact_us_button }}</a>
                                    {%- endif -%}
                                    </div>
                                {%- endif -%}
                                </div>
                            </div>
                        </div>
                    {%- endif -%}

                    {%- if settings.enable_multilang_shopify or settings.enable_currencies -%}
                        {%- if section.settings.show_language_currency -%}
                            <div class="header__languageCurrency header-language_currency d-inline-block p-relative ml-20">
                                <button type="button" class="icon-languageCurrency header__icon b-zero bg-none p-zero" aria-label="Language Currency" data-lang-currency>{% render 'icon-globe' %}</button>
                                <div class="top-language-currency p-absolute top-0 right-0 ml-0 zi-3 p-zero opacity-0 ft-0{% if settings.enable_multilang_shopify and settings.enable_currencies %} top-language-currency--all{% endif %}">
                                    {%- if settings.enable_multilang_shopify -%}
                                        {% render 'halo-top-language' %}
                                    {%- endif -%}

                                    {%- if settings.enable_currencies -%}
                                        {% render 'halo-top-currency' %}
                                    {%- endif -%}
                                </div>
                            </div>
                        {%- endif -%}
                    {%- endif -%}
                </div>
            </div>
            {%- if section.settings.header_style == 'style_2' -%}
                </div>
            {%- endif -%}
        </div>
        <div class="wrapper-header-bottom">
            <div class="container container-{{ section.settings.container }}">
                <div class="header-bottom d-flex pt-10 pb-10">
                    <div class="header-bottom-left">
                        <div class="header__logo">
                            {%- if menu_tab.size > 0 -%}
                                {% assign page_handle = page.handle %}
                                <div class="header__heading header__heading--item">
                                    {%- for block in menu_tab -%}
                                        {%- liquid
                                            assign multi_page_handle = pages[block.settings.link].handle
                                            assign multi_page_url = pages[block.settings.link].url
                                            assign class = 'header__heading-link focus-inset'
                                            if template.suffix contains 'landing'
                                                if page_handle == multi_page_handle
                                                    assign class = 'header__heading-link focus-inset is-active'
                                                endif
                                            else
                                                if forloop.first
                                                    assign class = 'header__heading-link focus-inset is-active'
                                                endif
                                            endif
                                            if multi_page_url != blank
                                                assign url = multi_page_url
                                            else
                                                assign url = routes.root_url
                                            endif
                                        -%}
                                        <a {% if url == blank %} role="link" aria-disabled="true"{% else %} href="{{ url }}"{% endif %} class="{{ class }}" style="--width_logo: {{ section.settings.logo_width | append: 'px' }}; --height_logo: auto" data-load-logo-page="{{ multi_page_handle }}"{% if block.settings.logo_mobile != blank %} data-logo-mobile="{{ block.settings.logo_mobile | img_url: 'master' }}" data-logo-mobile-alt="{{ block.settings.logo_mobile.alt | default: shop.name | escape }}"{% endif %}>
                                            {%- if block.settings.logo != blank -%}
                                                {%- assign image = block.settings.logo -%}
                                                {%- assign image_size = section.settings.logo_width | append: 'x' -%}
