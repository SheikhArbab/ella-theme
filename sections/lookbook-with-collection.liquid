<link rel="stylesheet" href="{{ 'component-lookbook-with-collection.css' | asset_url }}" media="print" onload="this.media='all'">
<noscript>{{ 'component-lookbook-with-collection.css' | asset_url | stylesheet_tag }}</noscript>
{{ 'lookbook-script.js' | asset_url | script_tag }}
{%- liquid 
    assign mg_top_desktop = section.settings.mg_top_desktop | append: 'px'
    assign mg_top_tablet = section.settings.mg_top_tablet | append: 'px'
    assign mg_top_mobile = section.settings.mg_top_mobile | append: 'px'
    assign mg_bottom_desktop = section.settings.mg_bottom_desktop | append: 'px'
    assign mg_bottom_tablet = section.settings.mg_bottom_tablet | append: 'px'
    assign mg_bottom_mobile = section.settings.mg_bottom_mobile | append: 'px'

    assign container = section.settings.container   
    assign padding_full_width = section.settings.padding_full_width | append: 'px'
    assign section_bg = section.settings.section_bg
    assign section_gradient = section.settings.section_gradient

    assign title = section.settings.title
    assign color_title = section.settings.color_title
    assign mg_bottom_title = section.settings.mg_bottom_title | append: 'px'
    assign fontsize_title = section.settings.fontsize_title | append: 'px'
    assign fontsize_title_mb = section.settings.fontsize_title_mb | append: 'px'

    assign des = section.settings.des
    assign color_des = section.settings.color_des
    assign margin_bottom_des = section.settings.margin_bottom_des | append: 'px'
    assign fontsize_des = section.settings.fontsize_des | append: 'px'
    assign fontsize_des_mb = section.settings.fontsize_des_mb | append: 'px'
    
    assign view_more = section.settings.view_more
    assign view_all_link = section.settings.view_all_link
    assign view_all_color = section.settings.view_all_color
    assign view_all_under_color = view_all_color | color_modify: 'alpha', 0.7
    assign view_all_enable_border = section.settings.view_all_enable_border
    assign view_all_font_size = section.settings.view_all_font_size | append: 'px'
    assign view_all_font_weight = section.settings.view_all_font_weight
    assign view_all_pos = section.settings.view_all_pos
    assign view_more_mg_top = section.settings.view_more_mg_top 

    assign title_align = section.settings.title_align
    assign content_width = section.settings.content_width | append: 'px'

    assign dot_desktop_size = section.settings.dot_desktop_size | append: 'px'
    assign dot_mobile_size = section.settings.dot_mobile_size | append: 'px'
    assign dot_shadow_color = section.settings.dot_shadow_color | color_modify: 'alpha', 0.5
    assign dot_outer_color = section.settings.dot_outer_color
    assign show_dot_number = section.settings.show_dot_number
    assign show_dot_number_mobile = section.settings.show_dot_number_mobile
    assign dot_inner_color = section.settings.dot_inner_color
    assign number_desktop_size = section.settings.number_desktop_size | append: 'px'
    assign number_mobile_size = section.settings.number_mobile_size | append: 'px'
    assign enable_lookbook_width = section.settings.enable_lookbook_width
    assign lookbook_width = section.settings.lookbook_width | append: 'px'
    assign lookbook_truncate = section.settings.lookbook_truncate
    assign lookbook_style = section.settings.lookbook_style

    assign show_text = section.settings.show_text
    assign hide_text = section.settings.hide_text
    assign show_btn_bg_color = section.settings.show_btn_bg_color
    assign show_btn_gradient = section.settings.show_btn_gradient
    assign show_btn_text_color = section.settings.show_btn_text_color
    assign show_btn_width = section.settings.show_btn_width | append: 'px'
    assign show_btn_height = section.settings.show_btn_height | append: 'px'

    assign has_fancy_box = false 
    for block in section.blocks 
        if block.settings.lookbook_image_action == 'popup_image'
            assign has_fancy_box = true 
        endif 
    endfor 

    assign scroll_height = section.settings.scroll_height
    assign scroll_track_bg = section.settings.scroll_track_bg
    assign scroll_thumb_color = section.settings.scroll_thumb_color
    assign scroll_thumb_hover_color = section.settings.scroll_thumb_hover_color

    assign link_text = section.settings.link_text
    assign link = section.settings.link
    assign link_underline = section.settings.link_underline
    assign link_color = section.settings.link_color
    assign link_color_hover = section.settings.link_color_hover
    assign link_margin_top = section.settings.link_margin_top | append: 'px'
    assign link_fs = section.settings.link_fs | append: 'px'
    assign link_fs_mb = section.settings.link_fs_mb | append: 'px'
    assign text_align = section.settings.text_align
    assign link_width = section.settings.link_width 
-%}
{%- if has_fancy_box -%}
    <link rel="stylesheet" href="{{ 'component-fancybox.css' | asset_url }}" media="print" onload="this.media='all'">
    <noscript>{{ 'component-fancybox.css' | asset_url | stylesheet_tag }}</noscript>
{%- endif -%}
<style type="text/css">
    .section-block-{{section.id}} {padding-top: {{ mg_top_desktop }};padding-bottom: {{ mg_bottom_desktop }}}
    {%- if container == 'fullwidth' -%}
        .section-block-{{section.id}} .container-full {padding-left: {{ padding_full_width }};padding-right: {{ padding_full_width }}}
    {%- endif -%}
    {%- if section_gradient != blank and section_gradient != 'rgba(0,0,0,0)' -%}
        .section-block-{{section.id}} {background: {{ section_gradient }}}
    {%- elsif section_bg != blank and section_bg != 'rgba(0,0,0,0)' -%}
        .section-block-{{section.id}} {background: {{ section_bg }}}
    {%- endif -%}
    @media screen and (max-width: 1024px) {.section-block-{{section.id}} {padding-top: {{ mg_top_tablet }};padding-bottom: {{ mg_bottom_tablet }}}}
    @media screen and (max-width: 767px) {.section-block-{{section.id}} {padding-top: {{ mg_top_mobile }};padding-bottom: {{ mg_bottom_mobile }}}}
</style>
<div class="collection-lookbook section-block-{{section.id}}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" id="collection-lookbook-{{section.id}}" style="--scrollbar-spacing: {{ view_more_mg_top | divided_by: 2 | append: 'px' }};--scrollbar-width: {{ scroll_height | append: 'px' }};--scrollbar-track-color: {% if scroll_track_bg != blank and scroll_track_bg != 'rgba(0,0,0,0)' %}{{ scroll_track_bg }}{% endif %};--scrollbar-thumb-color: {% if scroll_thumb_color != blank and scroll_thumb_color != 'rgba(0,0,0,0)' %}{{ scroll_thumb_color }}{% endif %};--scrollbar-thumb-hover-color: {% if scroll_thumb_hover_color != blank and scroll_thumb_hover_color != 'rgba(0,0,0,0)' %}{{ scroll_thumb_hover_color }}{% endif %}">
    <div class="{% if container == 'fullwidth' %}container-full{% elsif container == '1170' %}container-1170{% elsif container == '1770' %}container-1770{% else %}container{% endif %}">
        {%- if title != blank or des != blank -%}
            <div class="halo-block-header text-{{ title_align }}" style="--max-width: {{ content_width }}">
                {%- if title != blank -%}
                    <h3 class="title collection-lookbook-title{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--fade-in{% endif %}" style="{% if settings.banner_animation == 'effect_fade_up' %}--animation-order: 1;{% endif %}--title-color: {{ color_title }};--title-mg-bottom: {{ mg_bottom_title }};--title-fs: {{ fontsize_title }};--title-fs-mb: {{ fontsize_title_mb }};--max-width: {{ content_width }}"><span class="text">{{ title }}</span></h3>
                {%- endif -%}
                {%- if des != blank -%}
                    <div class="des collection-lookbook-des{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--fade-in{% endif %}" style="{% if settings.banner_animation == 'effect_fade_up' %}--animation-order: 2;{% endif %}--des-color: {{ color_des }};--des-mg-bottom: {{ margin_bottom_des }};--des-fs: {{ fontsize_des }};--des-fw-mb: {{ fontsize_des_mb }};--max-width: {{ content_width }}">{{ des }}</div>        
                {%- endif -%}
            </div>
        {%- endif -%} 
        {%- if section.blocks.size > 0 -%}
            <div class="halo-block-content">
                {%- assign count = 0 -%}
                {%- for block in section.blocks -%}
                    {%- assign count = count | plus: 1 -%}
                {%- endfor -%}
                <div class="lookbooks disable-srollbar-mb column-{{ count }}" data-show-lookbook-container style="--count: {{ count }}">
                    {%- for block in section.blocks -%}
                        {%- if block.type == 'lookbook_image' -%}
                            {%- liquid 
                                assign lookbook_image = block.settings.lookbook_image
                                assign lookbook_image_action = block.settings.lookbook_image_action
                                assign lookbook_image_link = block.settings.lookbook_image_link

                                assign product_1 = block.settings.product_1
                                assign left_1 = block.settings.horizontal_pos_1 | append: '%' 
                                assign top_1 = block.settings.vertical_pos_1 | append: '%' 
                                assign lookbook_pos_1 = block.settings.lookbook_pos_1

                                assign product_2 = block.settings.product_2
                                assign left_2 = block.settings.horizontal_pos_2 | append: '%' 
                                assign top_2 = block.settings.vertical_pos_2 | append: '%' 
                                assign lookbook_pos_2 = block.settings.lookbook_pos_2
    
                                assign product_3 = block.settings.product_3
                                assign left_3 = block.settings.horizontal_pos_3 | append: '%' 
                                assign top_3 = block.settings.vertical_pos_3 | append: '%' 
                                assign lookbook_pos_3 = block.settings.lookbook_pos_3
    
                                assign product_4 = block.settings.product_4
                                assign left_4 = block.settings.horizontal_pos_4 | append: '%' 
                                assign top_4 = block.settings.vertical_pos_4 | append: '%' 
                                assign lookbook_pos_4 = block.settings.lookbook_pos_4

                                assign link_text = block.settings.link_text
                                assign link = block.settings.link
                                assign link_underline = block.settings.link_underline
                                assign link_color = block.settings.link_color
                                assign link_color_hover = block.settings.link_color_hover
                                assign link_margin_top = block.settings.link_margin_top | append: 'px'
                                assign link_fs = block.settings.link_fs | append: 'px'
                                assign link_fs_mb = block.settings.link_fs_mb | append: 'px'
                                assign text_align = block.settings.text_align
                                assign link_width = block.settings.link_width 
                            -%}
                            <lookbook-with-show-all class="lookbook-image-item {% if enable_lookbook_width %}width-restricted{% endif %}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" data-lookbook-item-container style="{% if settings.banner_animation == 'effect_fade_up' %}--animation-order: {{ forloop.index }};{% endif %}--dot-desktop-size: {{ dot_desktop_size }};--dot-mobile-size: {{ dot_mobile_size }};--dot-shadow-color: {{ dot_shadow_color }};--dot-outer-color: {{ dot_outer_color }};--dot-inner-color: {{ dot_inner_color }};--dot-number-desktop-size: {{ number_desktop_size }};--dot-number-mobile-size: {{ number_mobile_size }};--max-width: {{ lookbook_width }};--des-max-width: {% if des_width > 0 %}{{ des_width | append: 'px' }}{% else %}unset{% endif %};--truncate-line: {{ lookbook_truncate }}">
                                <div class="lookbook-image" data-lookbook-image data-products-show-style="2">
                                    <div class="img-box{% if section.settings.image_behavior != 'none' %} animate--{{ section.settings.image_behavior }} o-hidden{% endif %}">
                                        {%- if lookbook_image != blank -%}
                                            <a class="adaptive_height fancybox image"{% if lookbook_image_action == 'link' %}{% if lookbook_image_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ lookbook_image_link }}"{% endif %}{% else %}class="fancybox" rel="lookbook" href="{{ lookbook_image | img_url: 'master' }}" data-fancybox="images"{% endif %} style="padding-top: {% unless lookbook_image.presentation.focal_point == '50.0% 50.0%' %}{{ 1 | divided_by: lookbook_image.aspect_ratio | times: 120 }}%{% else %}{{ 1 | divided_by: lookbook_image.aspect_ratio | times: 100 }}%{% endunless %}">
                                                <img {% render 'lazyload',preview_img: lookbook_image %}{% unless lookbook_image.presentation.focal_point == '50.0% 50.0%' %} style="object-position: {{ lookbook_image.presentation.focal_point }}"{% endunless %}/>
                                                {%- if settings.enable_lazyload -%}<span class="data-lazy-loading"></span>{%- endif -%}
                                            </a>
                                        {%- else -%}
                                            <div class="not_img" style="--height_not_img: 570px">420 x 570px</div>
                                        {%- endif -%}
                                    </div>
                                    {%- if product_1 != blank or product_2 != blank or product_3 != blank or product_4 != blank -%}
                                        <lookbook-wrapper>
                                        {%- if product_1 != blank -%}
                                            {% render 'lookbook-modal-wrapper', lookbook_style: lookbook_style, top: top_1, left: left_1, show_dot_number: show_dot_number, show_dot_number_mobile: show_dot_number_mobile, lookbook_pos: lookbook_pos_1, product: product_1, show_plus_icon: show_plus_icon, num: 1 %}
                                        {%- endif -%}
                                        {%- if product_2 != blank -%}
                                            {% render 'lookbook-modal-wrapper', lookbook_style: lookbook_style, top: top_2, left: left_2, show_dot_number: show_dot_number, show_dot_number_mobile: show_dot_number_mobile, lookbook_pos: lookbook_pos_2, product: product_2, show_plus_icon: show_plus_icon, num: 2 %}
                                        {%- endif -%}
                                        {%- if product_3 != blank -%}
                                            {% render 'lookbook-modal-wrapper', lookbook_style: lookbook_style, top: top_3, left: left_3, show_dot_number: show_dot_number, show_dot_number_mobile: show_dot_number_mobile, lookbook_pos: lookbook_pos_3, product: product_3, show_plus_icon: show_plus_icon, num: 3 %}
                                        {%- endif -%}
                                        {%- if product_4 != blank -%}
                                            {% render 'lookbook-modal-wrapper', lookbook_style: lookbook_style, top: top_4, left: left_4, show_dot_number: show_dot_number, show_dot_number_mobile: show_dot_number_mobile, lookbook_pos: lookbook_pos_4, product: product_4, show_plus_icon: show_plus_icon, num: 4 %}
                                        {%- endif -%}
                                        </lookbook-wrapper>
                                    {%- endif -%}

                                    {%- if product_1 != blank or product_2 != blank or product_3 != blank or product_4 != blank -%}
                                        <div class="show-all-lookbook" data-show-all-lookbook style="--view-all-bg: {% if show_btn_gradient != blank and show_btn_gradient != 'rgba(0,0,0,0)' %}{{ show_btn_gradient }}{% elsif show_btn_bg_color != blank and show_btn_bg_color != 'rgba(0,0,0,0)' %}{{ show_btn_bg_color }}{% endif %};--show-text-color: {% if show_btn_text_color != blank and show_btn_text_color != 'rgba(0,0,0,0)' %}{{ show_btn_text_color }}{% endif %};--button-width: {{ show_btn_width }};--button-height: {{ show_btn_height }}">
                                            <div class="btns-wrapper">
                                                <button class="show-btn-wrapper btn-wrapper" data-show-btn>{% render 'icon-plus' %} 
                                                    {%- if show_text != blank -%}
                                                        <span class="text">{{ show_text }}</span>
                                                    {%- endif -%}
                                                </button>
                                                <button class="hide-btn-wrapper btn-wrapper" data-hide-btn>{% render 'icon-close', class: 'icon icon-close' %}
                                                    {%- if hide_text != blank -%}
                                                        <span class="text">{{ hide_text }}</span>
                                                    {%- endif -%}
                                                </button>
                                            </div>
                                        </div>
                                    {%- endif -%}
                                    <div class="collection-wrapper">
                                        <button type="button" class="lookbook-show-all-close" data-lookbook-show-all-close role="button"><span class="visually-hidden">{{ 'general.common.close' | t }}</span>{% render 'icon-close-thin' %}</button>
                                        <div class="collection {% if true %}space-around{% else %}flex-start{% endif %}">
                                            {%- if product_1 != blank -%}
                                                <div class="product product-1">
                                                    <div class="product-left">
                                                        <a {% if product_1.url == blank %} role="link" aria-disabled="true"{% else %} href="{{ product_1.url }}"{% endif %} class="item-image"><img src="{{ product_1.featured_image.src | product_img_url: 'compact' }}" alt="{{ product_1.featured_image.alt | escape }}" /></a>
                                                    </div>
                                                    <div class="product-right">
                                                        <a {% if product_1.url == blank %} role="link" aria-disabled="true"{% else %} href="{{ product_1.url }}"{% endif %} class="product-title card-title link-underline"><span class="text">{{ product_1.title }}</span></a>
                                                        {% render 'price', product: product_1, price_class: '' %}
                                                    </div>
                                                </div>
                                            {%- endif -%}
                                            {%- if product_2 != blank -%}
                                                <div class="product product-2">
                                                    <div class="product-left">
                                                        <a {% if product_2.url == blank %} role="link" aria-disabled="true"{% else %} href="{{ product_2.url }}"{% endif %} class="item-image"><img src="{{ product_2.featured_image.src | product_img_url: 'compact' }}" alt="{{ product_2.featured_image.alt | escape }}" /></a>
                                                    </div>
                                                    <div class="product-right">
                                                        <a {% if product_2.url == blank %} role="link" aria-disabled="true"{% else %} href="{{ product_2.url }}"{% endif %} class="product-title card-title link-underline"><span class="text">{{ product_2.title }}</span></a>
                                                        {% render 'price', product: product_2, price_class: '' %}
                                                    </div>
                                                </div>
                                            {%- endif -%}
                                            {%- if product_3 != blank -%}
                                                <div class="product product-3">
                                                    <div class="product-left">
                                                        <a {% if product_3.url == blank %} role="link" aria-disabled="true"{% else %} href="{{ product_3.url }}"{% endif %} class="item-image"><img src="{{ product_3.featured_image.src | product_img_url: 'compact' }}" alt="{{ product_3.featured_image.alt | escape }}" /></a>
                                                    </div>
                                                    <div class="product-right">
                                                        <a {% if product_3.url == blank %} role="link" aria-disabled="true"{% else %} href="{{ product_3.url }}"{% endif %} class="product-title card-title link-underline"><span class="text">{{ product_3.title }}</span></a>
                                                        {% render 'price', product: product_3, price_class: '' %}
                                                    </div>
                                                </div>
                                            {%- endif -%}
                                            {%- if product_4 != blank -%}
                                                <div class="product product-4">
                                                    <div class="product-left">
                                                        <a {% if product_4.url == blank %} role="link" aria-disabled="true"{% else %} href="{{ product_4.url }}"{% endif %} class="item-image"><img src="{{ product_4.featured_image.src | product_img_url: 'compact' }}" alt="{{ product_4.featured_image.alt | escape }}" /></a>
                                                    </div>
                                                    <div class="product-right">
                                                        <a {% if product_4.url == blank %} role="link" aria-disabled="true"{% else %} href="{{ product_4.url }}"{% endif %} class="product-title card-title link-underline"><span class="text">{{ product_4.title }}</span></a>
                                                        {% render 'price', product: product_4, price_class: '' %}
                                                    </div>
                                                </div>
                                            {%- endif -%}
                                        </div>
                                    </div>
                                </div>
                                <a{% if link == blank %} role="link" aria-disabled="true"{% else %} href="{{ link }}"{% endif %} class="link collection-lookbook-link text-{{ text_align }}" style="--link-color: {% if link_color != blank and link_color != 'rgba(0,0,0,0)' %}{{ link_color }}{% endif %};--link-hover-color: {% if link_color_hover != blank and link_color_hover != 'rgba(0,0,0,0)' %}{{ link_color_hover }}{% endif %};--link-mg-top: {{ link_margin_top }};--link-fs: {{ link_fs }};--link-fs-mb: {{ link_fs_mb }};--link-max-width: {% if link_width > 0 %}{{ link_width | append: 'px' }}{% else %}unset{% endif %};{% if link_underline %}--link-underline-color: {% if link_color_hover != blank %}{{ link_color_hover }}{% endif %}{% endif %}">{{ link_text }}</a>
                            </lookbook-with-show-all>
                        {%- endif -%}
                    {%- endfor -%}
                </div>
            </div>
        {%- endif -%}
        {%- if view_more != blank -%}
            <div class="view-all-container text-{{ view_all_pos }}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--fade-in{% endif %}">
                <a class="view-more"{% if view_all_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ view_all_link }}"{% endif %} title="{{ view_more }}" style="{% if settings.banner_animation == 'effect_fade_up' %}--animation-order: 3;{% endif %}--view-color: {% if view_all_color != blank %}{{ view_all_color }}{% endif %};--view-fs: {{ view_all_font_size }};--view-fw: {{ view_all_font_weight }};--view-mg-top: {{ view_more_mg_top | divided_by: 2 | append: 'px' }};{% if view_all_enable_border %}text-decoration: underline;text-underline-offset: 2px;text-decoration-color: {% if view_all_under_color != blank and view_all_under_color != 'rgba(0,0,0,0)' %}{{ view_all_under_color }}{% else %}currentColor{% endif %}{% endif %}">{{ view_more }}</a>
            </div>
        {%- endif -%}
    </div>
</div>
<script>
    class LookbookWithShowAll extends HTMLElement {
        constructor() {
            super();
        }
        connectedCallback() {
            this.showBtn = this.querySelector('[data-show-btn]');
            this.hideBtn = this.querySelector('[data-hide-btn]');
            this.lookbookShowAllCloseBtn = this.querySelector('[data-lookbook-show-all-close]');
            this.lookbookImage = this.querySelector('[data-lookbook-image]');
            this.showBtn?.addEventListener('click', this.toggleShowAll.bind(this));
            this.lookbookShowAllCloseBtn?.addEventListener('click', this.toggleShowAll.bind(this));
        }
        toggleShowAll(e) {
            const button = e.currentTarget;
            this.lookbookImage.classList.toggle('show-all-products', button.hasAttribute('data-show-btn'));
        }
    }
    customElements.define('lookbook-with-show-all', LookbookWithShowAll);
</script>
{%- if has_fancy_box -%}
    <script src="{{ 'fancybox.js' | asset_url }}" defer="defer"></script>
{%- endif -%}
{% schema %}
  {
    "name": "t:sections.collection_lookbook.name",
    "settings": [   
        {
            "type": "header",
            "content": "t:settings_schema.general.name"
        },
        {
            "type": "select",
            "id": "container",
            "label": "t:sections.layout.container.option",
            "default": "container",
            "options": [
                {
                    "value": "container",
                    "label": "t:sections.layout.container.option1"
                },
                {
                    "value": "1170",
                    "label": "t:sections.layout.container.option4"
                },
                {
                    "value": "1770",
                    "label": "t:sections.layout.container.option3"
                },
                {
                    "value": "fullwidth",
                    "label": "t:sections.layout.container.option2"
                }
            ]
        },
        {
            "type": "range",
            "id": "padding_full_width",
            "label": "t:sections.layout.container.padding_full_width",
            "min": 0,
            "max": 60,
            "step": 5,
            "unit": "t:sections.layout.fontsize.fontsize_title.unit",
            "default": 0,
            "info": "t:sections.layout.container.info_full_width"
        },
        {
            "type" : "color",
            "id" : "section_bg",
            "label": "t:settings_schema.colors.settings.colors_name.background_color",
            "default" : "#fafafa"
        },
        {
            "type" : "color_background",
            "id" : "section_gradient",
            "label": "t:settings_schema.colors.settings.colors_name.background_color_gradient"
        },
        {
            "type": "header",
            "content": "t:sections.layout.margin_top"
        },
        {
            "type": "range",
            "id": "mg_top_desktop",
            "label": "t:sections.layout.mg_desktop.label",
            "min": 0,
            "max": 100,
            "step": 5,
            "unit": "t:sections.layout.mg_desktop.unit",
            "default": 50
        },
        {
            "type": "range",
            "id": "mg_top_tablet",
            "label": "t:sections.layout.mg_tablet.label",
            "min": 0,
            "max": 100,
            "step": 5,
            "unit": "t:sections.layout.mg_desktop.unit",
            "default": 50
        },
        {
