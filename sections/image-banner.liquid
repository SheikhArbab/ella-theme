            #halo-image-banner-{{ section.id }} .item--smallImg-st4 .banner-button {min-width: 140px}
            #halo-image-banner-{{ section.id }} .content-box--absolute-half .banner-sub-title{color: var(--color_sub_title_mb)}
            #halo-image-banner-{{ section.id }} .content-box--absolute-half .banner-sub-title--border span {border-bottom: 1px solid var(--color_sub_title_mb)}
            #halo-image-banner-{{ section.id }} .content-box--absolute-half .banner-heading{color: var(--heading_color_mb)}
            #halo-image-banner-{{ section.id }} .content-box--absolute-half .banner-text{color: var(--des_color_mb)}
            #halo-image-banner-{{ section.id }} .content-box--absolute-half .countdown-block {color: var(--color-countdown-mb-style)}
            #halo-image-banner-{{ section.id }} .content-box--absolute-half .clock-item,
            #halo-image-banner-{{ section.id }} .content-box--absolute-half .shape-square .clock-item,
            #halo-image-banner-{{ section.id }} .content-box--absolute-half .shape-square .clock-item:first-of-type {border-color: var(--color-countdown-mb-style)}
            #halo-image-banner-{{ section.id }} .item--largeImg .banner-text {margin-bottom: var(--margin_bottom_des_tab);}
            #halo-image-banner-{{ section.id }} .item--largeImg .banner-sub-title {margin-bottom: var(--margin_bottom_sub_title);}
            #halo-image-banner-{{ section.id }} .item--largeImg .banner-heading {padding-bottom: 0px;margin-bottom: 10px;font-size: var(--heading_font_size_tab);line-height: var(--heading_line_height_tab) !important}
        }
        @media (max-width: 991px){
            #halo-image-banner-{{ section.id }} .item--smallImg-st2 .flex-box__banner {flex-direction: column}
