                    {%- if section.settings.image_banner_title != blank -%} 
                        <h3 class="title{% unless section.settings.enable_border_title %} title--no-border{% endunless %}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" style="{% if settings.banner_animation == 'effect_fade_up' %}--animation-order: 0;{% endif %}--color_title: {{ color_title }};--fontsize_title:{{ fontsize_title | append: 'px' }};--fontsize_title_mb: {{ fontsize_title_mb | append: 'px' }};--line_height_title: {{ fontsize_title | plus: 8| append: 'px' }};--margin_bottom_title: {{ margin_bottom_title | append: 'px' }};"><span class="text">{{ section.settings.image_banner_title }}</span></h3>
                    {%- endif -%}
                    {%- if section.settings.image_banner_des != blank -%}
                        <div class="rte{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" style="{% if settings.banner_animation == 'effect_fade_up' %}--animation-order: 0;{% endif %}--color_des: {{ color_des }};--fontsize_des: {{ fontsize_des | append: 'px' }}">{{ section.settings.image_banner_des }}</div>        
                    {%- endif -%}
                </div>
