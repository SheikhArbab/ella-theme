        #halo-image-banner-{{ section.id }} .flex-box__banner .content-box {max-width: var(--content_width)}
        #halo-image-banner-{{ section.id }} .flex-box__banner .content-box__col-1 {margin-right: var(--content_spacing_right);margin-bottom: 0;max-width: var(--content_width);transform: translateY(var(--smallImg_text_center_pos))}
        .layout_rtl #halo-image-banner-{{ section.id }} .flex-box__banner .content-box__col-1 {margin-left: var(--content_spacing_right);margin-right: auto}
        #halo-image-banner-{{ section.id }} .flex-box__banner .content-box__col-2 {margin-left: var(--button_spacing_left);transform: translateY(var(--smallImg_btn_center_pos))}
