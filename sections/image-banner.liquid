            #halo-image-banner-{{ section.id }} .banner-sub-title {font-size: var(--font_size_sub_title)}
            #halo-image-banner-{{ section.id }} .item .banner-text {font-size: var(--des_font_size)}
            #halo-image-banner-{{ section.id }} .banner-button:hover{color: var(--button_color_hover);background: var(--button_bg_hover);border-color: var(--button_border_hover)}
            #halo-image-banner-{{ section.id }} .banner-button:hover svg {fill: var(--button_color_hover)}
            #halo-image-banner-{{ section.id }} .item--customImg + .item--customImg {margin-top: 0}
