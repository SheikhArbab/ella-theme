            #halo-image-banner-{{ section.id }} .content-box__col-1 {margin-right: 30px}
            #halo-image-banner-{{ section.id }} .content-box__col-2 {margin-left: 30px}
        }
        @media (max-width: 1199px) {
            #halo-image-banner-{{ section.id }} .column-2 .item--customImg {width: 100%}
            #halo-image-banner-{{ section.id }} .column-2 .type-item--custom-2 {margin-top: 30px}
        }
        @media (max-width: 1024px) {
            #halo-image-banner-{{ section.id }} .video-open-popup--ct-pos .video-button {top: var(--top-mb);left: var(--left-mb)}   
            #halo-image-banner-{{ section.id }} .video-open-popup svg, #halo-image-banner-{{ section.id }} .video-open-popup--icon-st2 svg {width: 44px;height: 44px}
            #halo-image-banner-{{ section.id }} .content-box__col-1--left,#halo-image-banner-{{ section.id }} .content-box__col-2--left {margin-right: auto}
            #halo-image-banner-{{ section.id }} .content-box__col-1--right, #halo-image-banner-{{ section.id }} .content-box__col-1--right { margin-left: auto}
            #halo-image-banner-{{ section.id }} .content-box__col-1--center, #halo-image-banner-{{ section.id }} .content-box__col-1--center { margin-right: auto;margin-left: auto}
