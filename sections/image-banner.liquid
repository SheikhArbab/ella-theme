            #halo-image-banner-{{ section.id }} .content-box--absolute-half .banner-heading {margin-bottom: 115px}
        }
        @media (min-width: 1200px) {#halo-image-banner-{{ section.id }}{padding-top: var(--mg_top_desktop);padding-bottom: var(--mg_bottom_desktop)}}
