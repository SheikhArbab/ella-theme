        #halo-image-banner-{{ section.id }} .banner-sub-title--border span {border-bottom: 1px solid var(--color_sub_title)}
        #halo-image-banner-{{ section.id }} .item .banner-text{color: var(--des_color);line-height: var(--line_height_des);margin-bottom: var(--margin_bottom_des)}
        #halo-image-banner-{{ section.id }} .type-item--special .banner-content,#halo-image-banner-{{ section.id }} .item--customImg .banner-content {background: var(--content_bg_color)}
        #halo-image-banner-{{ section.id }} .item--customImg .banner-content,#halo-image-banner-{{ section.id }} .type-item--special .banner-content {border:  1px solid var(--content_border_color)}
